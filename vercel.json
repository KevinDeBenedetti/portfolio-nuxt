{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm turbo run build",
  "installCommand": "pnpm install",
  "outputDirectory": ".output",
  "framework": "nuxtjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), interest-cohort=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "Expect-CT",
          "value": "max-age=86400, enforce"
        },
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" },
        { "key": "X-DNS-Prefetch-Control", "value": "on" },
        { "key": "X-Download-Options", "value": "noopen" },
        { "key": "X-Permitted-Cross-Domain-Policies", "value": "none" },
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://static.cloudflareinsights.com; style-src 'self' 'unsafe-inline' https://fonts.bunny.net; font-src 'self' https://fonts.bunny.net data:; img-src 'self' data: blob: https://images.unsplash.com https://*.githubusercontent.com https://*.cloudinary.com https://avatars.githubusercontent.com; connect-src 'self' https://api.nuxt.studio https://cloudflareinsights.com https://api.github.com; frame-ancestors 'self'; base-uri 'self'; form-action 'self'; object-src 'none'; upgrade-insecure-requests;"
        }
      ]
    },
    {
      "source": "/.well-known/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        { "key": "Expect-CT", "value": "max-age=86400, enforce" },
        { "key": "Cache-Control", "value": "public, max-age=86400" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/_nuxt/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        { "key": "Expect-CT", "value": "max-age=86400, enforce" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/_vercel/image(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        { "key": "Expect-CT", "value": "max-age=86400, enforce" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|woff2?|ttf|eot|svg|png|jpg|jpeg|gif|webp|ico)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        { "key": "Expect-CT", "value": "max-age=86400, enforce" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/__sitemap__/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        { "key": "Expect-CT", "value": "max-age=86400, enforce" },
        { "key": "Cache-Control", "value": "public, max-age=3600, s-maxage=86400" }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        { "key": "Expect-CT", "value": "max-age=86400, enforce" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/.env:path*",
      "destination": "/404"
    },
    {
      "source": "/.git/:path*",
      "destination": "/404"
    },
    {
      "source": "/.svn/:path*",
      "destination": "/404"
    },
    {
      "source": "/.htaccess",
      "destination": "/404"
    },
    {
      "source": "/.htpasswd",
      "destination": "/404"
    },
    {
      "source": "/.DS_Store",
      "destination": "/404"
    },
    {
      "source": "/.gitignore",
      "destination": "/404"
    },
    {
      "source": "/wp-:path*",
      "destination": "/404"
    },
    {
      "source": "/admin:path*",
      "destination": "/404"
    },
    {
      "source": "/phpmyadmin:path*",
      "destination": "/404"
    },
    {
      "source": "/config.php",
      "destination": "/404"
    },
    {
      "source": "/config.yml",
      "destination": "/404"
    },
    {
      "source": "/config.json",
      "destination": "/404"
    },
    {
      "source": "/package.json",
      "destination": "/404"
    },
    {
      "source": "/package-lock.json",
      "destination": "/404"
    },
    {
      "source": "/swagger:path*",
      "destination": "/404"
    },
    {
      "source": "/phpinfo:path*",
      "destination": "/404"
    },
    {
      "source": "/xmlrpc:path*",
      "destination": "/404"
    },
    {
      "source": "/server-status",
      "destination": "/404"
    },
    {
      "source": "/server-info",
      "destination": "/404"
    }
  ]
}
